"; DCS Config\r\n"
"[general]\r\n"
"debounce_ms = 10\r\n"
"max_text_length = 45\r\n"
"\r\n"
"[network]\r\n"
"dhcp = 0; not implemented\r\n"
"join_multicast = 0; not implemented\r\n"
"use_dcs_ip = 0; Use DCS computer IP\r\n"
"; Custom mac address\r\n"
"; Set 00:00:00:00:00:00 to use chip mac address\r\n"
"mac = 00:00:00:00:00:00\r\n"
"module_ip = 192.168.0.50; IP address for CH32V307 module\r\n"
"dcs_ip = 192.168.0.202; IP address for computer running DCS\r\n"
"subnet_mask = 255.255.255.0; netmask for your network\r\n"
"gateway = 192.168.0.1; gateway for your network\r\n"
"dcs_port = 7778; default DCS-BIOS port to receive control inputs\r\n"
"\r\n"
"[encoders]\r\n"
";Encoders require 4 parameters per encoder\r\n"
";Input A pin, input B pin, Clockwise message and Counter-Clockwire message\r\n"
"count = 1; Number of encoders configured\r\n"
"enc0_a_pin = PB0\r\n"
"enc0_b_pin = PB1\r\n"
"enc0_cw_text = NS430_RIGHT_SMALL_BTN_ROT -3200\r\n"
"enc0_ccw_text = NS430_RIGHT_SMALL_BTN_ROT +3200\r\n"
"\r\n"
"[buttons]\r\n"
";Buttons require 3 parameters per encoder\r\n"
";Pin number, pressed message and released message\r\n"
"count = 2; Number of buttons configured\r\n"
"btn0_pin = PE0\r\n"
"btn0_press_text = LDG_LIGHT_SW TOGGLE\r\n"
"btn0_release_text = MASTER_ARM_OFF\r\n"
"btn1_pin = PE1\r\n"
"btn1_press_text = FLAPS_UP\r\n"
"btn1_release_text = FLAPS_STOP\r\n"

;
